{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/images/(.*)", "destination": "/images/$1" },
    {
      "source": "/src/access/images/(.*)",
      "destination": "/src/access/images/$1"
    },
    { "source": "/posts/(.*)", "destination": "/posts/$1" }
  ]
}
